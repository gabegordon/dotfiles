[user]
	email = ggordon@persistentsystems.com
	name = Gabe Gordon
[rerere]
	enabled = 1
[color]
	ui = true
[push]
	default = matching
[github]
	user = gabegordon
[core]
	autocrlf = false
	safecrlf = true
	editor = vim
[alias]
	co = checkout
        ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	unstage = reset HEAD --
        fixdate = commit --amend --date="$(date -R)" 
        find = log --pretty=\"format:%Cgreen%H %Cblue%s\" --name-status --grep 
[core]
	excludesfile = ~/.gitignore_global
	#pager = diff-so-fancy | less --tabs=4 -RFX
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = yellow
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[diff-so-fancy]
	markEmptyLines = false
	stripLeadingSymbols = false
[pull]
	rebase = true
[magithub]
	online = false
[magithub "status"]
	includeStatusHeader = false
	includePullRequestsSection = false
	includeIssuesSection = false
[help]
	autocorrect = 15
[protocol "file"]
	allow = always
